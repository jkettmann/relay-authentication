{
  "name": "relay-authentication",
  "version": "0.1.0",
  "description": "An example showing role based authentication with relay",
  "main": "client/main.js",
  "scripts": {
    "start": "npm run relay-compiler:watch | babel-node start.js",
    "relay-compiler": "relay-compiler --src client --schema server/graphql/schema.graphql",
    "relay-compiler:watch": "relay-compiler --src client --schema server/graphql/schema.graphql --watch",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "test:server": "mocha --compilers js:babel-core/register --require server/test/testHelper.js 'server/test/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jkettmann/relay-authentication.git"
  },
  "keywords": [
    "skeleton",
    "template",
    "react",
    "quickstart"
  ],
  "author": "Johannes Kettmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkettmann/relay-authentication/issues"
  },
  "homepage": "https://github.com/jkettmann/relay-authentication",
